{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:a11y": "vitest run src/__tests__/accessibility/",
    "test:performance": "vitest run src/__tests__/performance/",
    "test:e2e": "vitest run src/__tests__/e2e/",
    "test:integration": "vitest run src/__tests__/integration/",
    "test:unit": "vitest run src/__tests__/unit/",
    "typecheck": "tsc --noEmit",
    "analyze": "vite-bundle-analyzer dist",
    "bundle-size-check": "bundlesize",
    "build-report": "vite build --mode analyze",
    "lighthouse": "lhci autorun",
    "audit-security": "npm audit --audit-level=moderate",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.cache",
    "validate": "npm run typecheck && npm run lint && npm run test:run",
    "setup:admin": "node scripts/setup-admin-simple.js"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.33.0",
    "@supabase/supabase-js": "^2.47.10",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.525.0",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "recharts": "^3.1.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "workbox-webpack-plugin": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^23.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^6.0.5",
    "vitest": "^3.2.4"
  }
}
