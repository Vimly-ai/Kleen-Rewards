# Production Environment Variables
# These should be overridden by Netlify environment variables

# Clerk Configuration - Use real production keys
# VITE_CLERK_PUBLISHABLE_KEY=your_production_clerk_key

# Supabase Configuration - Use real production credentials
# VITE_SUPABASE_URL=your_production_supabase_url
# VITE_SUPABASE_ANON_KEY=your_production_supabase_key

# Disable mock data in production
VITE_ENABLE_MOCK_DATA=false

# Enable PWA features
VITE_ENABLE_PWA=true

# Production environment
VITE_APP_ENV=production